{
	"name": "backend-graffinity",
	"version": "1.0.0",
	"description": "Graffinity",
	"author": "Drop Shot Divas",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"prisma:generate": "npx prisma generate",
		"prisma:dev": "npx prisma migrate dev",
		"prisma:reset": "npx prisma migrate reset",
		"prisma:deploy": "npx prisma migrate deploy",
		"prisma:studio": "npx prisma studio",
		"prisma:seed": "ts-node prisma/seed",
		"seed": "npx prisma db seed",
		"seed:prod": "node dist/src/seed",
		"seed:dev": "ts-node prisma/seed.ts",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/src/main",
		"start:resolve": "npx prisma migrate resolve --rolled-back \"20230101120000_V1_initial_schema\"",
		"start:migrate:prod": "npx prisma migrate deploy && npx prisma generate && npm run start:prod",
		"lint": "npx prettier --write .",
		"eslint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test:dotenv": "dotenv -e .env.test jest -i",
		"test": "jest -i",
		"test:cov": "jest --coverage",
		"test:watch": "jest --watch",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"prisma": {
		"schema": "prisma/schema.prisma",
		"introspect": true,
		"generate": true,
		"seed": "ts-node prisma/seed.ts",
		"seed:prod": "node dist/prisma/seed",
		"seed:dev": "ts-node prisma/seed.ts"
	},
	"dependencies": {
		"@nestjs/common": "^9.3.2",
		"@nestjs/config": "^2.3.0",
		"@nestjs/core": "^9.3.2",
		"@nestjs/jwt": "^10.0.1",
		"@nestjs/mapped-types": "*",
		"@nestjs/passport": "^9.0.1",
		"@nestjs/platform-express": "^9.3.2",
		"@nestjs/swagger": "^6.1.4",
		"@prisma/client": "^4.9.0",
		"@types/express": "^4.17.16",
		"argon2": "^0.30.3",
		"aws-sdk": "^2.1309.0",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"cookie-session": "^2.0.0",
		"dotenv": "^16.0.3",
		"exif-be-gone": "^1.3.2",
		"express-session": "^1.17.3",
		"geolocation": "^0.2.0",
		"jsonwebtoken": "^9.0.0",
		"moment": "^2.29.4",
		"nearestneighbour": "^0.1.1",
		"passport": "^0.6.0",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"piexifjs": "^1.0.6",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^4.1.2",
		"rxjs": "^7.8.0",
		"sharp": "^0.31.3",
		"sort-by-typescript": "^1.2.3",
		"ts-exif-parser": "^0.2.2",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@mermaid-js/mermaid-cli": "^9.3.0",
		"@nestjs/cli": "^9.3.0",
		"@nestjs/schematics": "^9.0.4",
		"@nestjs/testing": "^9.3.2",
		"@types/cookie-parser": "^1.4.3",
		"@types/cookie-session": "^2.0.44",
		"@types/express": "^4.17.16",
		"@types/express-session": "^1.17.5",
		"@types/jest": "28.1.8",
		"@types/multer": "^1.4.7",
		"@types/node": "^18.11.18",
		"@types/passport": "^1.0.11",
		"@types/passport-jwt": "^3.0.8",
		"@types/passport-local": "^1.0.35",
		"@types/sharp": "^0.31.1",
		"@types/supertest": "^2.0.11",
		"@types/uuid": "^9.0.0",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.50.0",
		"eslint": "^8.33.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "^28.1.3",
		"jest-mock-extended": "^3.0.1",
		"piexif-ts": "^2.0.16",
		"prettier": "^2.8.3",
		"prisma": "^4.9.0",
		"prisma-erd-generator": "^1.2.5",
		"source-map-support": "^0.5.20",
		"supertest": "^6.3.3",
		"ts-jest": "28.0.8",
		"ts-loader": "^9.4.2",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "4.1.2",
		"typescript": "^4.9.5"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
